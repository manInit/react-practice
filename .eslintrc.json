{
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": ["warn", "always"],
    "react/prop-types": 0,
    "no-unused-vars": [
      "warn", { 
      "vars": "all" 
      }
    ]
  },
  "extends": [
    "plugin:react/recommended"
  ]
}